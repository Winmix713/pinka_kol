    "prepare": "husky install || true",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "security": "npm audit --audit-level moderate",
    "check-updates": "npx npm-check-updates",
